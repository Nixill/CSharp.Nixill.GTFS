<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net8</TargetFramework>
    <PackageId>Nixill.GTFS</PackageId>
    <Version>$(RELEASE_VERSION)</Version>
    <Authors>Nixill</Authors>
    <PackageDescription>An extensible GTFS parsing library.</PackageDescription>
    <RepositoryUrl>https://github.com/ShadowFoxNixill/CSharp.Nixill.GTFS</RepositoryUrl>
    <RootNamespace>Nixill.GTFS</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NodaTime" Version="3.1.10" />
  </ItemGroup>

</Project>
